    "version": "0.2.0",
    "configurations": [
        {
            "name": "zanzeff_*_cal.py",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/applications/calibration",
            "args": [
                "--n-processes",
                "1",
                "--n-max-gen",
                "5",
                "--pop-size",
                "10",
                // "--debug",
            ]
        },
        {
            "name": "Python: zanzeff_*_cal_post.py",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}/applications/calibration",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/<executable file>",
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}